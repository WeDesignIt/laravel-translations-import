{
    "name": "wedesignit/laravel-translations-import",
    "description": "Import translations from your Laravel translation files to your database.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bodhi Looij",
            "email": "bodhilooij1998@gmail.com"
        },
        {
            "name": "Patrick van Kouteren",
            "email": "info@wedesignit.nl"
        },
        {
            "name": "Yannick Schrijvers",
            "email": "yincimaster@gmail.com"
        }
    ],
    "keywords": [
        "translations",
        "import",
        "laravel"
    ],
    "require": {
        "php": "^8.0"
    },
    "suggest": {
        "spatie/laravel-translation-loader": "Retrieve translations seemless from the database or language file"
    },
    "autoload": {
        "psr-4": {
            "WeDesignIt\\LaravelTranslationsImport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WeDesignIt\\LaravelTranslationsImport\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WeDesignIt\\LaravelTranslationsImport\\TranslationsImportServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "orchestra/testbench": "^3.8"
    }
}
